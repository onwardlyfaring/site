<div id="sidebar-contents" class="grid-cols-1 gap-4 pt-8 h-full grid">
    <div>
        <a href="/">
            <img alt="{{ siteconfig.imageAlt }}" src="{{ siteconfig.image }}" class="rounded-full h-36 border border-gray-500" height="144px" width="144px" />
        </a>
        <div class="text-center font-semibold pt-6 text-gray-500">
            <a href="/" class="mx-auto">{{ siteconfig.title }}</a>
        </div>
        <div class="text-center text-sm pt-3 px-8 text-gray-500">
            {{ siteconfig.description }}
        </div>
    </div>
    <div class="menu">
        <ul class="list-none pl-0">
            {# <li class="navitem text-center uppercase {{ page.url | isHomeLink('/') }}">
                <a href="/" class="block no-underline">Home</a>
            </li>             #}
            <li class="navitem text-center uppercase {{ page.url | isActiveLink('/posts') }}">
                <a href="/posts/" class="block no-underline">Notice</a>
            </li>
            <li class="navitem text-center uppercase {{ page.url | isActiveLink('/essays') }}">
                <a href="/essays/" class="block no-underline">Think Through</a>
            </li>
            <li class="navitem text-center uppercase {{ page.url | isActiveLink('/things-i-love') }}">
                <a href="/things-i-love/" class="block no-underline">Hold Dear</a>
            </li>
            <li class="navitem text-center uppercase {{ page.url | isActiveLink('/projects') }}">
                <a href="/projects/" class="block no-underline">Create</a>
            </li>
            <li class="navitem text-center uppercase {{ page.url | isActiveLink('/questions-and-answers') }}">
                <a href="/questions-and-answers/" class="block no-underline">Q&A</a>
            </li>
            <li class="navitem text-center uppercase {{ page.url | isActiveLink('/contact') }}">
                <a href="/contact/" class="block no-underline">Connect</a>
            </li>
        </ul>
    </div>
    
    <div class="grid grid-cols-1 gap-5 mx-auto place-content-start mb-12">
        {% if siteconfig.socialLinkedIn %}
        <a href="https://linkedin.com/in/{{ siteconfig.socialLinkedIn }}" aria-label="Open LinkedIn profile" target="_blank" class="w-5 h-5" rel="noopener">
            <svg role="img" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                <title>LinkedIn icon</title>
